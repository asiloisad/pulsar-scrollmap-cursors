{
  "name": "scrollmap-cursors",
  "description": "Cursor position markers for scrollmap",
  "repository": "https://github.com/asiloisad/pulsar-scrollmap-cursors",
  "keywords": [
    "scrollmap",
    "cursor",
    "marker"
  ],
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "providedServices": {
    "scrollmap": {
      "versions": {
        "1.0.0": "provideScrollmap"
      }
    }
  },
  "configSchema": {
    "showAll": {
      "order": 1,
      "type": "boolean",
      "title": "Show all cursors",
      "description": "Display all cursors or only the last one.",
      "default": true
    }
  }
}
